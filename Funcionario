package unicesumar.segundoBimestre;

public class Funcionario {
	private String nome;
	private int matricula;

	public Funcionario(String nome, int matricula) {
		this.nome = nome;
		this.matricula = matricula;
	}

	public String getNome() {
		return nome;
	}
	public float getMatricula() {
		return matricula;
	}

}
